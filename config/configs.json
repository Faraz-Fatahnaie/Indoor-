{
  "cuda": true,
  "dataset": {
    "name": "MIT",
    "n_channel": 3,
    "image_size": 256,
    "augmentation": 0,
    "n_worker": 4
  },
  "model": {
    "name": "resnet-18",
    "epoch": 200,
    "batch_size": 32,
    "loss": "CrossEntropyLoss",
    "optim": "Adam",
    "lr": 0.001,
    "weight_decay": 0.0001,
    "scheduler": "ReduceLROnPlateau",
    "min_lr": 0.0000001,
    "patience": 5,
    "early_stop": 10,
    "factor": 0.1,
    "gamma": 0.5,
    "regularization": "L2"
  }
}